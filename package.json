{
  "name": "miracle-shop-users-migration",
  "version": "1.0.0",
  "description": "Shopifyユーザーデータを取得してSupabaseに移行するツール",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "migrate": "npm run build && npm start",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "shopify",
    "supabase",
    "migration",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@shopify/shopify-api": "^11.4.0",
    "@supabase/supabase-js": "^2.47.10",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "winston": "^3.17.0",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

